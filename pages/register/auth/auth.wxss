.noLogin-view {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.noLogin-img {
  margin-top: 50px;
  margin-bottom: 30px;
}
.noLogin-text {
  color: #666666;
}
.noLogin-btn {
  margin-top: 30px;
  border-top-left-radius: 25px;
  border-bottom-left-radius: 25px;
  border-top-right-radius: 25px;
  border-bottom-right-radius: 25px;
  background-color: #f39e15;
  color: white;
  font-size: 12px;
  padding: 5px 100px;
  border: 1px solid #f39e15;
}