page {
  background-color: #f7f7f7;
}
.top {
  margin-left: 19px;
  margin-top: 20rpx;
  /* margin-bottom: 10rpx; */
  font-size: 30rpx;
  color: red;
}
.list {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding-left: 10px;
  padding-right: 10px;
}
.item {
  margin: 9px;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  background-color: white;
  border: 1px solid #e3e3e3;
  border-radius: 10rpx;
}
.bottom {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.bottom-title {
  font-size: 28rpx;
  color: #333333;
  margin-bottom: 10rpx;
}
.bottom-price {
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  width: 100%;
}
.bottom-price-right {
  font-size: 24rpx;
  color: #333333;
}