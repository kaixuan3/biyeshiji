page {
  background-color: #f7f7f7;
}
.searchView {
  background-color: white;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 2rpx solid #e3e3e3;
}
.search-inputh {
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  margin-left: 10px;
  margin-right: 10px;
  height: 50rpx;
  background-color: #f4f4f4;
  padding-left: 20rpx;
  border-radius: 25rpx;
  font-size: 30rpx;
  color: #333333;
}
.search-btn {
  margin-right: 10px;
  height: 50rpx;
  font-size: 30rpx;
  color: #333333;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-radius: 30rpx;
  border: 2rpx solid #e3e3e3;
}
.typeView {
  background-color: white;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 2rpx solid #e3e3e3;
}
.typeView-left {
  font-size: 30rpx;
  color: #333333;
  margin-left: 10px;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  height: 50rpx;
}
.typeView-right {
  font-size: 30rpx;
  color: #333333;
  margin-left: 10rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  height: 50rpx;
}

.list {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding-left: 10px;
  padding-right: 10px;
}
.item {
  margin: 9px;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  background-color: white;
  border: 1px solid #e3e3e3;
  border-radius: 10rpx;
}
.bottom {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.bottom-title {
  font-size: 28rpx;
  color: #333333;
  margin-bottom: 10rpx;
}
.bottom-price {
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  width: 100%;
}
.bottom-price-right {
  font-size: 24rpx;
  color: #333333;
}